<Window x:Class="EinrichtungsStatistik.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EinrichtungsStatistik"
        mc:Ignorable="d"
        Title="Einrichtungs-Statistik" Height="300" Width="483" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid>
        <Button x:Name="buttonDateneingabe" Content="Dateneingabe" HorizontalAlignment="Left" Margin="25,30,0,0" VerticalAlignment="Top" Width="115"/>
        <Button x:Name="buttonFragebogenentwurf" Content="Fragebogenentwurf" HorizontalAlignment="Left" Margin="25,70,0,0" VerticalAlignment="Top" Width="115" Click="buttonFragebogenentwurf_Click"/>
        <Button x:Name="buttonAuswertung" Content="Auswertung" HorizontalAlignment="Left" Margin="25,110,0,0" VerticalAlignment="Top" Width="115"/>
        <Button x:Name="buttonStammdaten" Content="Stammdaten" HorizontalAlignment="Left" Margin="25,150,0,0" VerticalAlignment="Top" Width="115"/>
        <Button x:Name="buttonBeenden" Content="Programm beenden" HorizontalAlignment="Left" Margin="25,190,0,0" VerticalAlignment="Top" Width="115" Click="buttonBeenden_Click"/>

    </Grid>
</Window>
