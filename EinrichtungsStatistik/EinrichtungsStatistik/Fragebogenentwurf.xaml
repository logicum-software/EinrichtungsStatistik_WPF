<Window x:Class="EinrichtungsStatistik.Fragebogenentwurf"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EinrichtungsStatistik"
        mc:Ignorable="d"
        Title="Fragebogenentwurf" Height="500" Width="800" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid>
        <Label Content="Ausgewählter Fragebogen" HorizontalAlignment="Left" Margin="25,20,0,0" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="179,24,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="200"/>
        <Button Content="Neu" HorizontalAlignment="Left" Margin="434,23,0,0" VerticalAlignment="Top" Width="75"/>
        <Button Content="Laden" HorizontalAlignment="Left" Margin="514,23,0,0" VerticalAlignment="Top" Width="75"/>
        <Button Content="Löschen" HorizontalAlignment="Left" Margin="594,23,0,0" VerticalAlignment="Top" Width="75" IsEnabled="False"/>
        <GroupBox Header="Enthaltene Fragen" HorizontalAlignment="Left" Height="350" Margin="25,74,0,25" VerticalAlignment="Top" Width="354"/>
        <GroupBox Header="Fragenkatalog" HorizontalAlignment="Left" Height="350" Margin="434,74,0,0" VerticalAlignment="Top" Width="333"/>
        <Button x:Name="buttonSchliessen" Content="Schliessen" HorizontalAlignment="Left" Margin="692,431,0,0" VerticalAlignment="Top" Width="75" Click="buttonSchliessen_Click"/>
    </Grid>
</Window>
